def_key "j"
    scroll_down
def_key "k"
    scroll_up

def_key "ctrl_u"
    page_up
def_key "ctrl_d"
    page_down

def_key "h"
    previous_column
def_key "l"
    next_column

def_key "."
    show_lyrics

def_key "n"
    next_found_item
def_key "N"
    previous_found_item

def_key "x"
    delete_playlist_items

def_key "0"
    volume_up
def_key "9"
    volume_down

def_key "z"
    jump_to_playing_song

def_key "g"
    move_home
def_key "G"
    move_end

def_key "D"
    clear_playlist
